  eval $_osbe_default_ar_files_var_name=\"\${$_osbe_default_ar_files_var_name-"$6"}\"
  eval $_osbe_default_ld_flags_var_name=\"\${$_osbe_default_ld_flags_var_name-"${12}"}\"

  _osbe_external_lib_dir_search_hint="$8"
  _osbe_external_lib_dir_search_opt_func="$9"
  _osbe_external_lib_dir_search_acknowledge_func="${10}"

  _osbe_external_lib_ask_for_root=1
  _osbe_external_lib_root="${13}"
  osbe_config_mech_read_value "${_osbe_external_lib_key_base}.root" \
                              _osbe_external_lib_root

  if test "$osbe_enable_guess_location" -eq "1" -a -n "$LOCATION_GUESS_PATH" -a \
          -n "$_osbe_external_lib_dir_search_hint" -a \
          -z "$_osbe_external_lib_root"
  then

    _osbe_external_lib_possible_locations=\
`osbe_guess_location "$_osbe_external_lib_name" \
                     "$LOCATION_GUESS_PATH" \
                     "$_osbe_external_lib_dir_search_hint" \
                     "$_osbe_external_lib_dir_search_opt_func" \
                     "$_osbe_external_lib_dir_search_acknowledge_func"`

  else

    _osbe_external_lib_possible_locations="$_osbe_external_lib_root"

  fi

  osbe_multiple_choice_lazy \
    "path to $_osbe_external_lib_name root directory" \
    "$_osbe_external_lib_name root path" \
    "$_osbe_external_lib_possible_locations" \
