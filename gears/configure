osbe_option_to_id()
{
  osbe_option_to_id__option_list=$1
  osbe_option_to_id__id_list=$2
  osbe_option_to_id__req_option=$3

  osbe_option_to_id__position=0
  for osbe_option_to_id__option in ${osbe_option_to_id__option_list}; do
    if test "$osbe_option_to_id__option" -eq "$osbe_option_to_id__req_option"; then
      break
    fi
    osbe_option_to_id__position=`expr $osbe_option_to_id__position + 1`
  done

  for osbe_option_to_id__id in ${osbe_option_to_id__id_list}; do
    if test "$osbe_option_to_id__position" -eq "0"; then
      $echo $osbe_option_to_id__id
      break
    fi
    osbe_option_to_id__position=`expr $osbe_option_to_id__position - 1`
  done
}

osbe_select_option()
{
  # IN
  osbe_select_option__id_list=$1
  osbe_select_option__descr_prefix=$2
  osbe_select_option__prompt=$3
  osbe_select_option__signature=$4
  osbe_select_option__default_id=$5
  osbe_select_option__suppress_id_desc=$6

  # BODY
  osbe_select_option__option_count=0
  osbe_select_option__option_list=""
  osbe_select_option__default_num="" # default option num corresponding to id
  for option in ${osbe_select_option__id_list}; do
    osbe_select_option__option_count=`expr $osbe_select_option__option_count + 1`
    osbe_select_option__option_list=\
"$osbe_select_option__option_list $osbe_select_option__option_count"
    if test "$osbe_select_option__default_id" = "$option"; then
      osbe_select_option__default_num="$osbe_select_option__option_count"
    fi
  done

  if test "$osbe_user_input_no_questions" = "0"; then
    $echo 1>&2
    for osbe_select_option__option in ${osbe_select_option__option_list}; do

      osbe_select_option__id=`osbe_option_to_id "$osbe_select_option__option_list" \
  "$osbe_select_option__id_list" "$osbe_select_option__option"`

      osbe_select_option__option_descr=`eval \$echo \
  '$'$osbe_select_option__descr_prefix$osbe_select_option__id`

      if test "$osbe_select_option__option" -eq "0$osbe_select_option__default_num"; then
        $echo "* \c" 1>&2
      else
        $echo "  \c" 1>&2
      fi

      if test -n "$osbe_select_option__descr_prefix"; then

        if test "$osbe_select_option__suppress_id_desc" = 1; then
          $echo "($osbe_select_option__option) $osbe_select_option__option_descr" \
          1>&2
        else
          $echo "($osbe_select_option__option) $osbe_select_option__option_descr \
  ($osbe_select_option__id)" 1>&2
        fi
      else
        $echo "($osbe_select_option__option) $osbe_select_option__id" 1>&2
      fi

    done
    $echo 1>&2

    osbe_select_option__input=0
    osbe_select_option__input_last=$osbe_select_option__option_count
    while test -z "$osbe_select_option__input" -o \
"0$osbe_select_option__input" -gt "0$osbe_select_option__input_last" -o \
"0$osbe_select_option__input" -lt 1; do
     $echo "$osbe_select_option__prompt \
[$osbe_select_option__default_num] : \c" 1>&2

     read osbe_select_option__input

     if test -z "$osbe_select_option__input" -a \
       -n "$osbe_select_option__default_num"; then
       osbe_select_option__input="$osbe_select_option__default_num"
     fi
    done
  else
    if test -z "$osbe_select_option__default_num" -o \
      "0$osbe_select_option__default_num" -gt "$osbe_select_option__option_count" -o \
      "0$osbe_select_option__default_num" -lt 1; then
      $echo 1>&2
      { { $as_echo "$as_me:$LINENO: error: invalid or empty value for $osbe_select_option__signature" >&5
$as_echo "$as_me: error: invalid or empty value for $osbe_select_option__signature" >&2;}
   { (exit 1); exit 1; }; }
    else
      osbe_select_option__input=$osbe_select_option__default_num
    fi
  fi

  osbe_option_to_id "$osbe_select_option__option_list" "$osbe_select_option__id_list" \
"$osbe_select_option__input"

}












#
# $1 = ask string prefix
# $2 = say string prefix
# $3 = known IDs list
# $4 = a prefix of the variables that contains signatures of the options
# $5 = resulting variable name
# $6 = config file key
# $7 = default value
# $8 = if "1" then id value will be suppressed in description
#
osbe_ask_for_option()
{
  eval res_value=\"\$$5\"

  _osbe_ask_for_option__tmp=""

  if test -n "$6"; then
    osbe_config_mech_read_value "$6" _osbe_ask_for_option__tmp
  fi

  if test -n "$_osbe_ask_for_option__tmp"; then
    _osbe_ask_for_option__default="$_osbe_ask_for_option__tmp";
  else
    if test -n "$res_value"; then
      _osbe_ask_for_option__default="$res_value"
    else
      _osbe_ask_for_option__default="$7"
    fi
  fi

#  _osbe_ask_for_option__default="$7"
#  if test -z "$res_value"; then
#    _osbe_ask_for_option__tmp=""
#    osbe_config_mech_read_value "$6" _osbe_ask_for_option__tmp
#    if test -n "$_osbe_ask_for_option__tmp"; then
#      _osbe_ask_for_option__default="$_osbe_ask_for_option__tmp";
#    fi
#  else
#    _osbe_ask_for_option__default="$res_value"
#  fi

  res_value=\
`osbe_select_option "$3" "$4" "$1" "$2" "$_osbe_ask_for_option__default" $8`

  if test $? -ne 0; then
    exit $?
  fi

  if test -n "$4"; then
    _osbe_ask_for_option__descr=`eval \$echo '$'$4$res_value`
  else
    _osbe_ask_for_option__descr=$res_value
  fi

  if test -n "$6"; then
    osbe_config_mech_write_value "$6" "$res_value"
  fi

  eval $5=\"$res_value\"

  { $as_echo "$as_me:$LINENO: $2 :  $_osbe_ask_for_option__descr" >&5
$as_echo "$as_me: $2 :  $_osbe_ask_for_option__descr" >&6;}
}

#
# Ask for Possibly Empty string
#
# $1 = ask string prefix
# $2 = say string prefix
# $3 = resulting variable name
# $4 = config file key
# $5 = edit if empty default (optional, 0 - default)
#
osbe_ask_for_pe_string()
{
  _osbe_ask_for_pe_string__ask_text="$1"
  _osbe_ask_for_pe_string__say_text="$2"
  _osbe_ask_for_pe_string__res_var_name="$3"
  _osbe_ask_for_pe_string__config_file_key="$4"
  _osbe_ask_for_pe_string__edit_if_empty_default="$5"

  if test "$#" -eq "5"; then
    _osbe_ask_for_pe_string__edit=\
"$_osbe_ask_for_pe_string__edit_if_empty_default"
  else
    _osbe_ask_for_pe_string__edit="0"
  fi

  if test -n "$_osbe_ask_for_pe_string__config_file_key"; then
    osbe_config_mech_read_value \
      "$_osbe_ask_for_pe_string__config_file_key" \
      "$_osbe_ask_for_pe_string__res_var_name"
  fi

  eval _osbe_ask_for_pe_string__res_var_value=\
\"\$$_osbe_ask_for_pe_string__res_var_name\"

  if test -n "$_osbe_ask_for_pe_string__res_var_value"; then
    _osbe_ask_for_pe_string__edit="0"
  fi

  if test "$osbe_user_input_no_questions" != "1"; then
    if test "$_osbe_ask_for_pe_string__edit" != "1"; then
      _osbe_ask_for_pe_string__choice=""
      while test "$_osbe_ask_for_pe_string__choice" = ""; do
        $echo 1>&2

        eval _osbe_ask_for_pe_string__res_var_value=\
\"\$$_osbe_ask_for_pe_string__res_var_name\"

        prompt="${_osbe_ask_for_pe_string__ask_text} \
($_osbe_ask_for_pe_string__res_var_value) [Y/e]: "

        if test ${#prompt} -gt 80; then
          prompt="${_osbe_ask_for_pe_string__ask_text}
($_osbe_ask_for_pe_string__res_var_value) [Y/e]: "
        fi

        $echo "${prompt}\c" 1>&2

        _osbe_ask_for_pe_string__choice_input=""
        read _osbe_ask_for_pe_string__choice_input
        if test -z "$_osbe_ask_for_pe_string__choice_input" -o \
          "$_osbe_ask_for_pe_string__choice_input" = "y" -o \
          "$_osbe_ask_for_pe_string__choice_input" = "Y"; then
          _osbe_ask_for_pe_string__choice="y"
        elif test "$_osbe_ask_for_pe_string__choice_input" = "e" -o \
          "$_osbe_ask_for_pe_string__choice_input" = "E"; then
          _osbe_ask_for_pe_string__choice="e"
        else
          $echo \
            "error: invalid choice (type 'y' to confirm or 'e' to edit)" 1>&2
        fi
      done
    else
      $echo 1>&2
      $echo "$_osbe_ask_for_pe_string__ask_text :" 1>&2
      _osbe_ask_for_pe_string__choice="e"
    fi

    if test "$_osbe_ask_for_pe_string__choice" = "e"; then
#      $echo "> \c" 1>&2
      $readline "> " $_osbe_ask_for_pe_string__res_var_name
    fi
  fi

  eval _osbe_ask_for_pe_string__res_var_value=\
\"\$$_osbe_ask_for_pe_string__res_var_name\"

  if test -n "$_osbe_ask_for_pe_string__config_file_key"; then
    osbe_config_mech_write_value \
      "$_osbe_ask_for_pe_string__config_file_key" \
      "$_osbe_ask_for_pe_string__res_var_value"
  fi

  { $as_echo "$as_me:$LINENO: $_osbe_ask_for_pe_string__say_text : $_osbe_ask_for_pe_string__res_var_value" >&5
$as_echo "$as_me: $_osbe_ask_for_pe_string__say_text : $_osbe_ask_for_pe_string__res_var_value" >&6;}
}

#
# Ask for Non Empty string
#
# $1 = ask string prefix
# $2 = say string prefix
# $3 = resulting variable name
# $4 = config file key
#
osbe_ask_for_ne_string()
{
  _osbe_ask_for_ne_string__ask_text="$1"
  _osbe_ask_for_ne_string__say_text="$2"
  _osbe_ask_for_ne_string__res_var_name="$3"
  _osbe_ask_for_ne_string__config_file_key="$4"

  eval $_osbe_ask_for_ne_string__res_var_name=\
\"\$$_osbe_ask_for_ne_string__res_var_name\"

  if test -n "$_osbe_ask_for_ne_string__config_file_key"; then
    osbe_config_mech_read_value \
      "$_osbe_ask_for_ne_string__config_file_key" \
      "$_osbe_ask_for_ne_string__res_var_name"
  fi

  eval _osbe_ask_for_ne_string__res_var_value=\
\"\$$_osbe_ask_for_ne_string__res_var_name\"

  if test "$osbe_user_input_no_questions" = "1"; then
    if test -z "$_osbe_ask_for_ne_string__res_var_value"; then
      $echo 1>&2
      { { $as_echo "$as_me:$LINENO: error: unable to figure out value for $_osbe_ask_for_ne_string__res_var_name" >&5
$as_echo "$as_me: error: unable to figure out value for $_osbe_ask_for_ne_string__res_var_name" >&2;}
   { (exit 1); exit 1; }; }
    fi
  else
    if test -n "$_osbe_ask_for_ne_string__res_var_value"; then
      _osbe_ask_for_ne_string__choice=""
      while test "$_osbe_ask_for_ne_string__choice" = ""; do
        $echo 1>&2

        prompt="${_osbe_ask_for_ne_string__ask_text} \
($_osbe_ask_for_ne_string__res_var_value) [Y/e]: "

        if test ${#prompt} -gt 80; then
          prompt="${_osbe_ask_for_ne_string__ask_text}
($_osbe_ask_for_ne_string__res_var_value) [Y/e]: "
        fi

        $echo "${prompt}\c" 1>&2

        _osbe_ask_for_ne_string__choice_input=""
        read _osbe_ask_for_ne_string__choice_input
        if test -z "$_osbe_ask_for_ne_string__choice_input" -o \
	   "$_osbe_ask_for_ne_string__choice_input" = "y" -o \
           "$_osbe_ask_for_ne_string__choice_input" = "Y"; then
	  _osbe_ask_for_ne_string__choice="y"
        elif test "$_osbe_ask_for_ne_string__choice_input" = "e" -o \
	     "$_osbe_ask_for_ne_string__choice_input" = "E"; then
	  _osbe_ask_for_ne_string__choice="e"
        else
	  $echo "error: invalid choice (type 'y' to confirm or 'e' to edit)" \
                1>&2
        fi
      done
    else
      $echo 1>&2
      $echo "$_osbe_ask_for_ne_string__ask_text :" 1>&2
      _osbe_ask_for_ne_string__choice="e"
    fi

    if test "$_osbe_ask_for_ne_string__choice" = "e" -o -z \
       "$_osbe_ask_for_ne_string__res_var_value"; then

      _osbe_ask_for_ne_string__res_var_value=""

      while test -z "$_osbe_ask_for_ne_string__res_var_value"; do
#        $echo "> \c" 1>&2
        $readline "> " _osbe_ask_for_ne_string__res_var_value
        if test -z "$_osbe_ask_for_ne_string__res_var_value"; then
	  $echo "error: empty string is not allowed" 1>&2
        fi
      done
    fi
  fi

  if test -n "$_osbe_ask_for_ne_string__config_file_key"; then
    osbe_config_mech_write_value "$_osbe_ask_for_ne_string__config_file_key" \
                                 "$_osbe_ask_for_ne_string__res_var_value"
  fi

  eval $_osbe_ask_for_ne_string__res_var_name=\
\"$_osbe_ask_for_ne_string__res_var_value\"

  { $as_echo "$as_me:$LINENO: $_osbe_ask_for_ne_string__say_text : $_osbe_ask_for_ne_string__res_var_value" >&5
$as_echo "$as_me: $_osbe_ask_for_ne_string__say_text : $_osbe_ask_for_ne_string__res_var_value" >&6;}
}

#
# Ask for Confirmation
#
# $1 = ask string prefix
# $2 = say string prefix
# $3 = resulting variable name
#
osbe_ask_for_confirmation()
{
  _osbe_ask_for_confirmation__ask_text="$1"
  _osbe_ask_for_confirmation__say_text="$2"

  if test "$osbe_user_input_no_questions" != "1"; then
    _osbe_ask_for_confirmation__ask="no"
    _osbe_ask_for_confirmation__choice=""
    while test "$_osbe_ask_for_confirmation__choice" = ""; do
      $echo 1>&2
      $echo "$_osbe_ask_for_confirmation__say_text" 1>&2
      $echo "$_osbe_ask_for_confirmation__ask_text [Y/e]: \c" 1>&2

      _osbe_ask_for_confirmation__choice_input=""
      read _osbe_ask_for_confirmation__choice_input
      if test -z "$_osbe_ask_for_confirmation__choice_input" -o \
        "$_osbe_ask_for_confirmation__choice_input" = "y" -o \
        "$_osbe_ask_for_confirmation__choice_input" = "Y"; then
        _osbe_ask_for_confirmation__choice="y"
      elif test "$_osbe_ask_for_confirmation__choice_input" = "e" -o \
        "$_osbe_ask_for_confirmation__choice_input" = "E"; then
        _osbe_ask_for_confirmation__choice="e"
      else
        $echo "error: invalid choice (type 'y' to confirm or 'e' to edit)" 1>&2
      fi
    done
  else
    _osbe_ask_for_confirmation__choice="y"
  fi

  eval $3=\"$_osbe_ask_for_confirmation__choice\"
}

#
# Non-interactive functions
#

#
# Set Possibly Empty string
# $1 is a name of the variable that will accept value
# $2 is a key name (e.g. "cxx.id")
#
osbe_set_pe_string()
{
  eval $1=\"\$$1\"
  osbe_config_mech_read_value "$2" "$1"

  eval _osbe_set_pe_string__value=\"\$$1\"
  osbe_config_mech_write_value "$2" "$_osbe_set_pe_string__value"
}

#
# Set Non Empty string
# $1 is a name of the variable that will accept value
# $2 is a key name (e.g. "cxx.id")
#
osbe_set_ne_string()
{
  eval $1=\"\$$1\"
  osbe_config_mech_read_value "$2" "$1"

  eval _osbe_set_pe_string__value=\"\$$1\"

  if test -z "$_osbe_set_pe_string__value"; then
    $echo 1>&2
    { { $as_echo "$as_me:$LINENO: error: unable to figure out value for $1" >&5
$as_echo "$as_me: error: unable to figure out value for $1" >&2;}
   { (exit 1); exit 1; }; }
  fi

  osbe_config_mech_write_value "$2" "$_osbe_set_pe_string__value"
}

#
# Adds option for confirmation/edition in subsequent osbe_ask_for_options call.
# $1 name of variable which should store option value
# $2 default value; can be overriden by corresponding value from default.config
# $3 is a key name (e.g. "server.paths.data")
# $4 option name as should appear in the interactive menu
# $5 option name as should appear in per option edit prompt; optional - if not
#    set will be used as specified in previous argument
# $6 if set to 1, then option can not be empty string; optional - 0 by default
# $7 option comment; it will be written into current.config if supplied
#
osbe_add_option()
{
  eval $1="\"$2\""; osbe_config_mech_read_value "$3" $1
  eval "_osbe_ask_for_options__options=\"\$_osbe_ask_for_options__options\\n  $4: \$$1\""

  if test -z "$5"; then
    _osbe_add_option__option_desc="\$_osbe_ask_for_options__title: $4 option"
  else
    _osbe_add_option__option_desc="$5"
  fi

  if test -n "$7"; then
    _osbe_add_option__write_comment="osbe_config_mech_write_comment \"$7\"; "
  else
    _osbe_add_option__write_comment=
  fi

  if test "$6" = "1"; then
    _osbe_add_option__edit=\
"${_osbe_add_option__write_comment}osbe_ask_for_pe_string \"Please enter $_osbe_add_option__option_desc\" \"$5\" $1 \"$3\""
  else
    _osbe_add_option__edit=\
"${_osbe_add_option__write_comment}osbe_ask_for_ne_string \"Please enter $_osbe_add_option__option_desc\" \"$5\" $1 \"$3\" \"1\""
  fi

  if test -z "$_osbe_ask_for_options__edit"; then
    _osbe_ask_for_options__edit="$_osbe_add_option__edit"
  else
    _osbe_ask_for_options__edit=\
"$_osbe_ask_for_options__edit; $_osbe_add_option__edit"
  fi

  _osbe_add_option__leave_as_is=\
"${_osbe_add_option__write_comment}osbe_config_mech_write_value \"$3\" \"\$$1\""

  if test -z "$_osbe_ask_for_options__leave_as_is"; then
    _osbe_ask_for_options__leave_as_is="$_osbe_add_option__leave_as_is"
  else
    _osbe_ask_for_options__leave_as_is=\
"$_osbe_ask_for_options__leave_as_is; $_osbe_add_option__leave_as_is"
  fi
}

# Proposes options (specified by previous osbe_add_option calls)
# for confirmation/edition.
# $1 menu title
# $2 menu prompt; optional - if ommited, then predefined prompt will be shown
#
osbe_ask_for_options()
{
  _osbe_ask_for_options__title=$1
  _osbe_ask_for_options__options="$1:$_osbe_ask_for_options__options"

  _osbe_ask_for_options__options_confirmation=""

  if test -z "$2"; then
    _osbe_ask_for_options__prompt="Please confirm or edit options listed above"
  else
    _osbe_ask_for_options__prompt="$2"
  fi

  osbe_ask_for_confirmation "$_osbe_ask_for_options__prompt" \
                            "$_osbe_ask_for_options__options" \
                             _osbe_ask_for_options__options_confirmation

  if test "$_osbe_ask_for_options__options_confirmation" = "e"; then
    eval "$_osbe_ask_for_options__edit"
  else
    eval "$_osbe_ask_for_options__leave_as_is"
  fi

  _osbe_ask_for_options__options=""
  _osbe_ask_for_options__edit=""
  _osbe_ask_for_options__leave_as_is=""
}

#
# $1 = ask string prefix
# $2 = say string prefix
# $3 = known IDs list
# $4 = a prefix of the variables that contains signatures of the options
# $5 = resulting variable name
# $6 = config file key (only for writting into it)
# $7 = "1" if ask for user input if no options, if "2" ne string is required

osbe_multiple_choice ()
{
  _osbe_multiple_choice__ask_anyway="1"

  _osbe_multiple_choice__result=""

  case `osbe_list_size "$3"` in
    0)
      _osbe_multiple_choice__ask_anyway="0"
      ;;
    1)
      eval _osbe_multiple_choice__result=\"\$$4`osbe_list_item "$3" 0`\"
      { $as_echo "$as_me:$LINENO: ${2} : $_osbe_multiple_choice__result" >&5
$as_echo "$as_me: ${2} : $_osbe_multiple_choice__result" >&6;}
      ;;
    *)
      eval "${4}Other"="Other"
      osbe_ask_for_option "Please select $1" \
                          "$2" \
                          "$3 Other" \
                          "$4" \
                          _osbe_multiple_choice__result \
                          "" \
                          "" \
                          "1"
      if test "$_osbe_multiple_choice__result" = "Other"; then
        _osbe_multiple_choice__result=""
      else
        eval _osbe_multiple_choice__result=\
"\"\$$4$_osbe_multiple_choice__result\""
      fi
    ;;
  esac

  if test -z "$_osbe_multiple_choice__result"; then
    case "$7" in
      1)
        osbe_ask_for_pe_string "Please enter $1" \
                               "$2" \
                               _osbe_multiple_choice__result \
                               "" \
                               "1"
        ;;
      2)
        osbe_ask_for_ne_string "Please enter $1" \
                               "$2" \
                               _osbe_multiple_choice__result
        ;;
      *)
        if test "$_osbe_multiple_choice__ask_anyway" = "1"; then
          osbe_ask_for_pe_string "Please enter $1" \
                                 "$2" \
                                 _osbe_multiple_choice__result \
                                 "" \
                                 "1"
        else
          { $as_echo "$as_me:$LINENO: ${2} :" >&5
$as_echo "$as_me: ${2} :" >&6;}
        fi
        ;;
    esac
  fi

  if test -n "$6"; then
    osbe_config_mech_write_value "$6" "$_osbe_multiple_choice__result"
  fi

  eval $5=\"\$_osbe_multiple_choice__result\"
}

#
# $1 = ask string prefix
# $2 = say string prefix
# $3 = list of values to choose from
# $4 = resulting variable name
# $5 = config file key (only for writting into it)
# $6 = "1" if ask for user input if no options, if "2" ne string is required

osbe_multiple_choice_lazy ()
{
  _osbe_multiple_choice_lazy__number="0"
  _osbe_multiple_choice_lazy__numbers=""

  for _osbe_multiple_choice_lazy__value in $3; do
    _osbe_multiple_choice_lazy__number=\
`expr $_osbe_multiple_choice_lazy__number + 1`
    _osbe_multiple_choice_lazy__numbers=\
"$_osbe_multiple_choice_lazy__numbers$_osbe_multiple_choice_lazy__number "
    eval _osbe_multiple_choice_lazy__list$_osbe_multiple_choice_lazy__number=\
\"\$_osbe_multiple_choice_lazy__value\"
  done

  osbe_multiple_choice "$1" \
                       "$2" \
                       "$_osbe_multiple_choice_lazy__numbers" \
                       _osbe_multiple_choice_lazy__list \
                       "$4" \
                       "$5" \
                       "$6"
}

