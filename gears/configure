    fi

    if test -n "$_osbe_external_ar_files"; then
      _osbe_external_lib_options="$_osbe_external_lib_options\
\n  extra linker flag(s) : $_osbe_external_ld_flags"
    fi

    if test -n "$_osbe_external_lib_options"; then
      _osbe_external_lib_options="$2 Options:$_osbe_external_lib_options"

