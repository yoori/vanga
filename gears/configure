#
# Default config information persistance/access mechanisms
#

osbe_config_mech_signature="default config info"
osbe_config_mech_in_default_file="default.config"
osbe_config_mech_in_file=""
osbe_config_mech_out_file="current.config"

osbe_config_mech_in_works="0"
osbe_config_mech_out_works="0"


# Check whether --with-config-file was given.
if test "${with_config_file+set}" = set; then
  withval=$with_config_file;
if test "$with_config_file" = "no"; then
  osbe_config_mech_in_file=""
elif test -z "$with_config_file"; then
  osbe_config_mech_in_file="$osbe_config_mech_in_default_file"
  if test -f $osbe_config_mech_in_file -a -r $osbe_config_mech_in_file; then
    osbe_config_mech_in_works="1"
  fi
else
  osbe_config_mech_in_file="$with_config_file"
  if test -f $osbe_config_mech_in_file -a -r $osbe_config_mech_in_file; then
    osbe_config_mech_in_works="1"
  else
    { $as_echo "$as_me:$LINENO: WARNING: $osbe_config_mech_signature : file specified does not exist \
or not readable : $osbe_config_mech_in_file" >&5
$as_echo "$as_me: WARNING: $osbe_config_mech_signature : file specified does not exist \
or not readable : $osbe_config_mech_in_file" >&2;}
  fi
fi

else

osbe_config_mech_in_file="$osbe_config_mech_in_default_file"
if test -f $osbe_config_mech_in_file -a -r $osbe_config_mech_in_file; then
  osbe_config_mech_in_works="1"
fi

fi


if test "$osbe_config_mech_in_file" = "$osbe_config_mech_out_file"; then
  { { $as_echo "$as_me:$LINENO: error: $osbe_config_mech_signature : input and output config files \
is the same file : $osbe_config_mech_in_file" >&5
$as_echo "$as_me: error: $osbe_config_mech_signature : input and output config files \
is the same file : $osbe_config_mech_in_file" >&2;}
   { (exit 1); exit 1; }; }
fi

if test "$osbe_config_mech_in_works" = "1"; then
  { $as_echo "$as_me:$LINENO: $osbe_config_mech_signature : input config file : \
$osbe_config_mech_in_file" >&5
$as_echo "$as_me: $osbe_config_mech_signature : input config file : \
$osbe_config_mech_in_file" >&6;}
fi

$echo "# $osbe_config_mech_signature : generated automatically by configure." \
