osbe_cxx_use_lib_libraries()
{
  osbe_cxx_use_lib_libraries__libs=
  for osbe_cxx_use_lib_libraries__word in $1; do
    osbe_cxx_use_lib_libraries__modified=`$echo $osbe_cxx_use_lib_libraries__word | tr -d '/'`
    if test "$osbe_cxx_use_lib_libraries__word" = "$osbe_cxx_use_lib_libraries__modified" ; then
      osbe_cxx_use_lib_libraries__prefix=-l
    else
      osbe_cxx_use_lib_libraries__prefix=
    fi
    osbe_cxx_use_lib_libraries__libs="$osbe_cxx_use_lib_libraries__libs $osbe_cxx_use_lib_libraries__prefix$osbe_cxx_use_lib_libraries__word"
  done
  $echo "$osbe_cxx_use_lib_libraries__libs"
}

osbe_cxx_use_lib_rules_gen()
{
  osbe_cxx_use_lib_rules_gen__rules_file_name="$1"
  osbe_cxx_use_lib_rules_gen__cpp_flags=`osbe_prefix_words "-I" "$2"`
  osbe_cxx_use_lib_rules_gen__ld_flags=`osbe_prefix_words "-L" "$3"`
  osbe_cxx_use_lib_rules_gen__ld_flags="$osbe_cxx_use_lib_rules_gen__ld_flags $7"
  osbe_cxx_use_lib_rules_gen__so_libs=`osbe_cxx_use_lib_libraries "$4"`
  osbe_cxx_use_lib_rules_gen__ar_libs=`osbe_cxx_use_lib_libraries "$5"`

  osbe_cxx_use_lib_rules_gen__rules_file=\
"./OpenSBE/config/CXX/$osbe_cxx_use_lib_rules_gen__rules_file_name"

  osbe_cxx_use_lib_rules_gen__guard=\
`$echo "$osbe_cxx_use_lib_rules_gen__rules_file_name"|tr './[:lower:]' '__[:upper:]'`

  osbe_cxx_use_lib_rules_gen__rules_content=`cat <<EOF
# @file $osbe_cxx_use_lib_rules_gen__rules_file_name
#
# !!! DO NOT EDIT !!!
# This file generated automatically by 'configure' script.

ifndef $osbe_cxx_use_lib_rules_gen__guard
define $osbe_cxx_use_lib_rules_gen__guard
1
endef

  ifeq (\\\$(origin rg_first_level), undefined)
    rg_first_level:=yes
  endif

  ifneq ("$osbe_cxx_use_lib_rules_gen__so_libs", "")
    rg_ar_only := no \\\$(rg_ar_only)
  endif

  ifeq (\\\$(rg_first_level), yes)
    rg_first_level:=no
$6
    rg_first_level:=yes

    rg_rules_dep_from += \\\$\\\$(osbe_builddir)/config/CXX/\
$osbe_cxx_use_lib_rules_gen__rules_file_name
  else
$6
  endif

  ifneq ("$osbe_cxx_use_lib_rules_gen__so_libs", "")
    rg_ar_only := \\\$(wordlist 2, 100000, \\\$(rg_ar_only))
  endif

  CPP_FLAGS := $osbe_cxx_use_lib_rules_gen__cpp_flags \\\$(CPP_FLAGS)

  ifeq (\\\$(and \\\$(strip $osbe_cxx_use_lib_rules_gen__ar_libs),\\\$(rg_ar_only)),)
    LD_FLAGS := $osbe_cxx_use_lib_rules_gen__ld_flags \\\$(LD_FLAGS)
  endif

  ifneq ("$osbe_cxx_use_lib_rules_gen__so_libs", "")
    LIBS := $osbe_cxx_use_lib_rules_gen__so_libs \\\$(LIBS)
  endif

  ifneq ("$osbe_cxx_use_lib_rules_gen__ar_libs", "")
    ifeq (,\\\$(rg_ar_only))
      LIBS := $osbe_cxx_use_lib_rules_gen__ar_libs \\\$(LIBS)
    endif
  endif
endif # $osbe_cxx_use_lib_rules_gen__guard

EOF`

  osbe_cxx_use_lib_rules_gen__rules_dir=\
`dirname $osbe_cxx_use_lib_rules_gen__rules_file`

  { as_dir=$osbe_cxx_use_lib_rules_gen__rules_dir
  case $as_dir in #(
  -*) as_dir=./$as_dir;;
  esac
  test -d "$as_dir" || { $as_mkdir_p && mkdir -p "$as_dir"; } || {
    as_dirs=
    while :; do
      case $as_dir in #(
      *\'*) as_qdir=`$as_echo "$as_dir" | sed "s/'/'\\\\\\\\''/g"`;; #'(
      *) as_qdir=$as_dir;;
      esac
      as_dirs="'$as_qdir' $as_dirs"
      as_dir=`$as_dirname -- "$as_dir" ||
$as_expr X"$as_dir" : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X"$as_dir" : 'X\(//\)[^/]' \| \
	 X"$as_dir" : 'X\(//\)$' \| \
	 X"$as_dir" : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X"$as_dir" |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
      test -d "$as_dir" && break
    done
    test -z "$as_dirs" || eval "mkdir $as_dirs"
  } || test -d "$as_dir" || { { $as_echo "$as_me:$LINENO: error: cannot create directory $as_dir" >&5
$as_echo "$as_me: error: cannot create directory $as_dir" >&2;}
   { (exit 1); exit 1; }; }; }

  $echo "$osbe_cxx_use_lib_rules_gen__rules_content" > \
    $osbe_cxx_use_lib_rules_gen__rules_file
}

#
# osbe_cxx_use_lib function. Plugs external C++ libraries into a project.
#
#  $1 - project wide unique library identifier. Used to generate keys for
#       current.config file.
#  $2 - verbose library name.
#  $3 - space separated list of "include" paths relative to library root
#       directory. Optional.
#  $4 - space separated list of "library" paths relative to library root
#       directory. Optional.
#  $5 - space separated list of shared libraries. Optional.
#  $6 - space separated list of static libraries. Optional.
#  $7 - produced rule file relative against $(osbe_builddir)/config/CXX/
#       path name.
#  $8 - library root directory search hint. Optional. If not specified OSBE
#       will not search.
#  $9 - name of function which returns additional search options (in "find"
#       utility format) being provided with search hint file relative path.
#       Optional.
# $10 - name of function which acknowledges search hint file found. Optional.
#       If not specified files will be acknowledged automatically.
# $11 - list of includes of other generated external pre.rules files. Optional.
# $12 - extra ld flags. Optional.
# $13 - initial value for root directory variable. Optional.

osbe_cxx_use_lib()
{
  _osbe_external_lib_id=\
`$echo "$1" | tr "\134/ +.[:upper:]" "_____[:lower:]"`

  eval called="\"\$${_osbe_external_lib_id}_guard\""

  if test "$called" == "1"; then
    return
  fi

  eval ${_osbe_external_lib_id}_guard="1"

  _osbe_external_lib_key_base="external.lib.${_osbe_external_lib_id}"
  _osbe_rule_key_base="${_osbe_external_lib_key_base}.cxx"

  _osbe_var_prefix=\
`$echo "$_osbe_external_lib_id" | tr "[:lower:]" "[:upper:]"`

  _osbe_default_include_var_name="${_osbe_var_prefix}_CXX_INCLUDE"
  _osbe_default_lib_var_name="${_osbe_var_prefix}_CXX_LIB"
  _osbe_default_so_files_var_name="${_osbe_var_prefix}_CXX_SO_FILES"
  _osbe_default_ar_files_var_name="${_osbe_var_prefix}_CXX_AR_FILES"
  _osbe_default_ld_flags_var_name="${_osbe_var_prefix}_CXX_LD_FLAGS"

#  echo "====> : _osbe_default_include_var_name=$_osbe_default_include_var_name"

  eval _osbe_external_lib_include="\"\$$_osbe_default_include_var_name\""
  eval _osbe_external_lib_dir="\"\$$_osbe_default_lib_var_name\""

  _osbe_external_lib_name="$2"

  eval $_osbe_default_include_var_name=\"\${$_osbe_default_include_var_name-"$3"}\"
  eval $_osbe_default_lib_var_name=\"\${$_osbe_default_lib_var_name-"$4"}\"
