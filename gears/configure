                                "$_osbe_external_lib_options" \
                                _osbe_external_lib_options_confirmation
    else
      _osbe_external_lib_options_confirmation="e"
    fi
  else
    _osbe_external_lib_options_confirmation="e"
  fi

  if test "$_osbe_external_lib_options_confirmation" = "e"; then
#
# Options editing required
#
    osbe_ask_for_pe_string \
      "Please enter $2 include directory path(s)" \
      "$2 include directory path(s)" \
      _osbe_external_lib_include \
      "${_osbe_rule_key_base}.include"

    osbe_ask_for_pe_string \
      "Please enter $2 library directory path(s)" \
      "$2 library directory path(s)" \
      _osbe_external_lib_dir \
      "${_osbe_rule_key_base}.lib"

    osbe_ask_for_pe_string \
      "Please enter $2 shared library file name(s)" \
      "$2 shared library file name(s)" \
      _osbe_external_so_files \
      "${_osbe_rule_key_base}.so_files"

    osbe_ask_for_pe_string \
      "Please enter $2 static library file name(s)" \
      "$2 static library file name(s)" \
      _osbe_external_ar_files \
      "${_osbe_rule_key_base}.ar_files"

    osbe_ask_for_pe_string \
      "Please enter $2 extra linker flag(s)" \
      "$2 extra linker flag(s)" \
      _osbe_external_ld_flags \
      "${_osbe_rule_key_base}.ld_flags"

  else
    osbe_config_mech_write_value "${_osbe_rule_key_base}.include" \
                                 "$_osbe_external_lib_include"

    osbe_config_mech_write_value "${_osbe_rule_key_base}.lib" \
                                 "$_osbe_external_lib_dir"

    osbe_config_mech_write_value "${_osbe_rule_key_base}.so_files" \
                                 "$_osbe_external_so_files"

    osbe_config_mech_write_value "${_osbe_rule_key_base}.ar_files" \
                                 "$_osbe_external_ar_files"

    osbe_config_mech_write_value "${_osbe_rule_key_base}.ld_flags" \
                                 "$_osbe_external_ld_flags"
  fi

  osbe_library_path "$_osbe_external_lib_dir"

  osbe_cxx_use_lib_rules_gen "$7" "$_osbe_external_lib_include" \
      "$_osbe_external_lib_dir" "$_osbe_external_so_files" \
      "$_osbe_external_ar_files" "${11}" "$_osbe_external_ld_flags"
}

#
# OSBE_CXX_USE_LIB macros. Plugs external C++ libraries into a project.
# Just calls osbe_cxx_use_lib function.
#
#  $1 - project wide unique library identifier. Used to generate keys for
#       current.config file.
#  $2 - verbose library name.
#  $3 - space separated list of "include" paths relative to library root
#       directory. Optional.
#  $4 - space separated list of "library" paths relative to library root
#       directory. Optional.
#  $5 - space separated list of shared libraries. Optional.
#  $6 - space separated list of static libraries. Optional.
#  $7 - produced rule file relative against $(osbe_builddir)/config/CXX/
#       path name.
#  $8 - library root directory search hint. Optional. If not specified OSBE
#       will not search.
#  $9 - name of function which returns additional search options (in "find"
#       utility format) being provided with search hint file relative path.
#       Optional.
# $10 - name of function which acknowledges search hint file found. Optional.
#       If not specified files will be acknowledged automatically.
# $11 - extra ld flags. Optional.
# $12 - initial value for root directory variable. Optional.






































