# Transforms name of the lib into its id
# ${1} - name of the lib
# ${2} - var name to put id of the lib into
osbe_cxx_lib_name()
{
  _osbe_cxx_lib_name_id=\
`$echo "${1}" | tr "\134/ +.\-[:upper:]" "______[:lower:]"`
  eval ${2}="\"\${_osbe_cxx_lib_name_id}\""
}

# Defines the cycle guard for the lib and check for
# its previous definition (i.e. cycle status)
# ${1} - lib id
# ${2} - def name or empty to clear
osbe_cxx_def_cycle()
{
  _osbe_cxx_def_cycle__name="_osbe_cxx_def_cycle__guard_${1}"
  if test -n "${2}"; then
    eval _osbe_cxx_def_cycle__called="\"\${${_osbe_cxx_def_cycle__name}}\""
    if test -n "${_osbe_cxx_def_cycle__called}"; then
      { { $as_echo "$as_me:$LINENO: error: \"${2}.def : cyclic dependece\"" >&5
$as_echo "$as_me: error: \"${2}.def : cyclic dependece\"" >&2;}
   { (exit 1); exit 1; }; }
    fi
  fi
  eval ${_osbe_cxx_def_cycle__name}="\"\${2}\""
}

# Searches for and executing the external def file.
# ${1} - def file name
# ${2} - library id
osbe_cxx_use_external()
{
  #search for def file in some directories
  _osbe_cxx_use_external__def_file=""
  for _osbe_cxx_use_external__file in `find ${_osbe_cxx_use_def__def_path} \
-name "${1}.def" 2>/dev/null`; do
    if test -n "${_osbe_cxx_use_external__file}" -a \
-f "${_osbe_cxx_use_external__file}" -a \
-r "${_osbe_cxx_use_external__file}"; then
      _osbe_cxx_use_external__def_file="${_osbe_cxx_use_external__file}"
      break
    fi
  done

  if test -z "${_osbe_cxx_use_external__def_file}"; then
    { { $as_echo "$as_me:$LINENO: error: \"${_osbe_cxx_execute_def__current_name} : \
${1}.def : library def file is not found\"" >&5
$as_echo "$as_me: error: \"${_osbe_cxx_execute_def__current_name} : \
${1}.def : library def file is not found\"" >&2;}
   { (exit 1); exit 1; }; }
  fi

  #execute it
  { $as_echo "$as_me:$LINENO: parsing external ${1}.def..." >&5
$as_echo "$as_me: parsing external ${1}.def..." >&6;}

  osbe_cxx_execute_def "${2}" "${_osbe_cxx_use_external__def_file}"

  eval _osbe_cxx_use_external__name="\"\${_osbe_cxx_lib__${2}_name}\""
  if test -z "${_osbe_cxx_use_external__name}"; then
    { { $as_echo "$as_me:$LINENO: error: \"${_osbe_cxx_execute_def__current_name} : ${1}.def : \
not a library definition\"" >&5
$as_echo "$as_me: error: \"${_osbe_cxx_execute_def__current_name} : ${1}.def : \
not a library definition\"" >&2;}
   { (exit 1); exit 1; }; }
  fi

  #create pre.rules file
  eval _osbe_cxx_use_external__root="\"\${_osbe_cxx_lib__${2}_root}\""
  eval _osbe_cxx_use_external__include="\"\${_osbe_cxx_lib__${2}_include}\""
  eval _osbe_cxx_use_external__lib="\"\${_osbe_cxx_lib__${2}_lib}\""
  eval _osbe_cxx_use_external__so_files="\"\${_osbe_cxx_lib__${2}_so_files}\""
  eval _osbe_cxx_use_external__ar_files="\"\${_osbe_cxx_lib__${2}_ar_files}\""
  eval _osbe_cxx_use_external__ld_flags="\"\${_osbe_cxx_lib__${2}_ld_flags}\""
  eval _osbe_cxx_use_external__hint="\"\${_osbe_cxx_lib__${2}_hint}\""
  eval _osbe_cxx_use_external__find_options=\
"\"\${_osbe_cxx_lib__${2}_find_options}\""
  eval _osbe_cxx_use_external__find_acknowledge=\
"\"\${_osbe_cxx_lib__${2}_find_acknowledge}\""
  eval _osbe_cxx_use_external__deps="\"\${_osbe_cxx_lib__${2}_deps}\""
  osbe_cxx_use_lib "${1}" "${_osbe_cxx_use_external__name}" \
"${_osbe_cxx_use_external__include}" "${_osbe_cxx_use_external__lib}" \
"${_osbe_cxx_use_external__so_files}" "${_osbe_cxx_use_external__ar_files}" \
"External/${1}.pre.rules" "${_osbe_cxx_use_external__hint}" \
"${_osbe_cxx_use_external__find_options}" \
"${_osbe_cxx_use_external__find_acknowledge}" \
"${_osbe_cxx_use_external__deps}" "${_osbe_cxx_use_external__ld_flags}" \
"${_osbe_cxx_use_external__root}"

  eval _osbe_cxx_defs_${2}_name="\"\${1}\""
  eval _osbe_cxx_defs_${2}_reference=\
"\"ifndef OSBE_MAKE_CLEAN_GOAL
include \\\$(osbe_builddir)/config/CXX/External/\${1}.pre.rules
endif\""
}

# Adds dependence to the list of dependences of the current library
# ${1} - dependence
osbe_cxx_add_dependence()
{
  eval _osbe_cxx_add_dependence__value=\
"\"\${_osbe_cxx_lib__${_osbe_cxx_execute_def__current_id}_deps}\""
  if test -z "${_osbe_cxx_add_dependence__value}"; then
    eval _osbe_cxx_lib__${_osbe_cxx_execute_def__current_id}_deps=\
"\"\${1}\""
  else
    eval _osbe_cxx_lib__${_osbe_cxx_execute_def__current_id}_deps=\
"\"\${_osbe_cxx_add_dependence__value}
\${1}\""
  fi
}

# Pushes the options, executes the specified def file,
# saves the options to the corresponding variables, pops the options
# ${1} - id of def
# ${2} - full name of def file
osbe_cxx_execute_def()
{
  if test -z "${_osbe_cxx_execute_def__current_name}"; then
    _osbe_cxx_execute_def__referrer="OSBE_CXX_DEF() : "
  else
    _osbe_cxx_execute_def__referrer="${_osbe_cxx_execute_def__current_name} : "
  fi
  if test ! -e "${2}"; then
    { { $as_echo "$as_me:$LINENO: error: \"${_osbe_cxx_execute_def__referrer}${2} : \
does not exist\"" >&5
$as_echo "$as_me: error: \"${_osbe_cxx_execute_def__referrer}${2} : \
does not exist\"" >&2;}
   { (exit 1); exit 1; }; }
  fi
  if test ! -f "${2}"; then
    { { $as_echo "$as_me:$LINENO: error: \"${_osbe_cxx_execute_def__referrer}${2} : \
is not a plain file\"" >&5
$as_echo "$as_me: error: \"${_osbe_cxx_execute_def__referrer}${2} : \
is not a plain file\"" >&2;}
   { (exit 1); exit 1; }; }
  fi
  if test ! -r "${2}"; then
    { { $as_echo "$as_me:$LINENO: error: \"${_osbe_cxx_execute_def__referrer}${2} : \
is not accessible\"" >&5
$as_echo "$as_me: error: \"${_osbe_cxx_execute_def__referrer}${2} : \
is not accessible\"" >&2;}
   { (exit 1); exit 1; }; }
  fi

  _osbe_cxx_execute_def__options="root name include lib so_files ar_files \
ld_flags hint find_options find_acknowledge \
_osbe_cxx_execute_def__current_id _osbe_cxx_execute_def__current_name"

  if test -z "${_osbe_cxx_execute_def__order}"; then
    _osbe_cxx_execute_def__order="1"
  else
    _osbe_cxx_execute_def__order=`expr ${_osbe_cxx_execute_def__order} + 1`
  fi

  for _osbe_cxx_execute_def__option in ${_osbe_cxx_execute_def__options}; do
    eval _osbe_cxx_execute_def__old="\"\${${_osbe_cxx_execute_def__option}}\""
    eval \
_osbe_cxx_execute_def__saved_${_osbe_cxx_execute_def__option}_${_osbe_cxx_execute_def__order}=\
"\"\${${_osbe_cxx_execute_def__option}}\""
    eval ${_osbe_cxx_execute_def__option}=
  done

  _osbe_cxx_execute_def__current_id="${1}"
  _osbe_cxx_execute_def__current_name="${2}"

  source "${2}"

  for _osbe_cxx_execute_def__option in ${_osbe_cxx_execute_def__options}; do
    eval _osbe_cxx_execute_def__value="\"\${${_osbe_cxx_execute_def__option}}\""
    eval _osbe_cxx_lib__${1}_${_osbe_cxx_execute_def__option}=\
"\"${_osbe_cxx_execute_def__value}\""
    eval ${_osbe_cxx_execute_def__option}=\
"\"\${_osbe_cxx_execute_def__saved_${_osbe_cxx_execute_def__option}_${_osbe_cxx_execute_def__order}}\""
  done

  _osbe_cxx_execute_def__order=`expr ${_osbe_cxx_execute_def__order} - 1`
}

# Entry point for the def files dependence.
# Checks if it was previously referenced.
# If it was, uses the cached dependence.
# Otherwise tries to execute it (if it's internal or external).
# Adds dependence to the list.
# ${1} - name of def file
osbe_cxx_dep()
{
  osbe_cxx_lib_name "${1}" _osbe_cxx_dep__id

  eval _osbe_cxx_dep__reference=\
"\"\${_osbe_cxx_defs_${_osbe_cxx_dep__id}_reference}\""

  if test -z "${_osbe_cxx_dep__reference}"; then
    eval _osbe_cxx_dep__name=\
"\"\${_osbe_cxx_defs_${_osbe_cxx_dep__id}_name}\""
    if test -z "${_osbe_cxx_dep__name}"; then
      osbe_cxx_def_cycle "${_osbe_cxx_dep__id}" "${1}"
      osbe_cxx_use_external "${1}" "${_osbe_cxx_dep__id}"
      osbe_cxx_lib_name "${1}" _osbe_cxx_dep__id
      osbe_cxx_def_cycle "${_osbe_cxx_dep__id}" ""
    else
      osbe_cxx_parse_def "${_osbe_cxx_dep__id}"
      osbe_cxx_lib_name "${1}" _osbe_cxx_dep__id
    fi
    eval _osbe_cxx_dep__reference=\
"\"\${_osbe_cxx_defs_${_osbe_cxx_dep__id}_reference}\""
  else
    eval _osbe_cxx_dep__name=\
"\"\${_osbe_cxx_defs_${_osbe_cxx_dep__id}_name}\""
    if test "${_osbe_cxx_dep__name}" != "${1}"; then
      { { $as_echo "$as_me:$LINENO: error: \"${_osbe_cxx_execute_def__current_name} : ${1} : \
previously referenced as ${_osbe_cxx_dep__name}\"" >&5
$as_echo "$as_me: error: \"${_osbe_cxx_execute_def__current_name} : ${1} : \
previously referenced as ${_osbe_cxx_dep__name}\"" >&2;}
   { (exit 1); exit 1; }; }
    fi
  fi

  if test "${_osbe_cxx_dep__reference}" = "none"; then
    { { $as_echo "$as_me:$LINENO: error: \"${_osbe_cxx_execute_def__current_name} : ${1} : \
referencing non-library def file\"" >&5
$as_echo "$as_me: error: \"${_osbe_cxx_execute_def__current_name} : ${1} : \
referencing non-library def file\"" >&2;}
   { (exit 1); exit 1; }; }
  fi

  osbe_cxx_add_dependence "${_osbe_cxx_dep__reference}"

  eval _osbe_cxx_dependence_${_osbe_cxx_execute_def__current_id}_ids=\
"\"\${_osbe_cxx_dependence_${_osbe_cxx_execute_def__current_id}_ids} \
\${_osbe_cxx_dep__id}\""
}

# Adds dependence of the feature
# ${1} - feature name
osbe_cxx_feature_dep()
{
  _osbe_cxx_feature_dep__feature=`$echo ${1} | tr "[:upper:]" "[:lower:]"`
  case "${_osbe_cxx_feature_dep__feature}" in
    corba)
      _osbe_cxx_feature_dep__pre=\
"include \$(osbe_builddir)/config/CXX/Corba.pre.rules"
      _osbe_cxx_feature_dep__post=\
"include \$(osbe_builddir)/config/CXX/Corba.post.rules"
      ;;
    xsdtree)
      _osbe_cxx_feature_dep__pre=\
"include \$(osbe_builddir)/config/CXX/XML/XSD.pre.rules"
      _osbe_cxx_feature_dep__post=\
"include \$(osbe_builddir)/config/CXX/XML/XSDTree.post.rules"
      ;;
    xsdparser)
      _osbe_cxx_feature_dep__pre=\
"include \$(osbe_builddir)/config/CXX/XML/XSD.pre.rules"
      _osbe_cxx_feature_dep__post=\
"include \$(osbe_builddir)/config/CXX/XML/XSDParser.post.rules"
      ;;
    orm)
      _osbe_cxx_feature_dep__pre=\
"include \$(osbe_builddir)/config/CXX/DB/Orm.pre.rules"
      _osbe_cxx_feature_dep__post=\
"include \$(osbe_builddir)/config/CXX/DB/Orm.post.rules"
      ;;
    protobuf)
      _osbe_cxx_feature_dep__pre=\
"include \$(osbe_builddir)/config/CXX/DR/ProtoBuf.pre.rules"
      _osbe_cxx_feature_dep__post=\
"include \$(osbe_builddir)/config/CXX/DR/ProtoBuf.post.rules"
      ;;
    *)
      { { $as_echo "$as_me:$LINENO: error: \"${_osbe_cxx_execute_def__current_name} : ${1} : \
unknown feature dependence\"" >&5
$as_echo "$as_me: error: \"${_osbe_cxx_execute_def__current_name} : ${1} : \
unknown feature dependence\"" >&2;}
   { (exit 1); exit 1; }; }
      ;;
  esac

  osbe_cxx_add_dependence "${_osbe_cxx_feature_dep__pre}"

  eval _osbe_cxx_lib__${_osbe_cxx_execute_def__current_id}_post=\
"\"\${_osbe_cxx_feature_dep__post}
\${_osbe_cxx_lib__${_osbe_cxx_execute_def__current_id}_post}\""
}

# Initializes variables for osbe_cxx_parse_def function
# ${1} - id of the def file
osbe_cxx_parse_vars()
{
  eval _osbe_cxx_parse_def__path=\
"\"\${_osbe_cxx_defs_${1}_path}\""
  eval _osbe_cxx_parse_def__name=\
"\"\${_osbe_cxx_defs_${1}_name}\""
  eval _osbe_cxx_parse_def__target=\
"\"\${srcdir}/\${_osbe_cxx_parse_def__path}\${_osbe_cxx_parse_def__name}.def\""
}

# Executes the def file and initialize the corresponding variables for it.
# ${1} - id of the def file
osbe_cxx_parse_def()
{
  eval _osbe_cxx_parse_def_execution__reference=\
"\"\${_osbe_cxx_defs_${1}_reference}\""
  if test -n "${_osbe_cxx_parse_def_execution__reference}"; then
    return
  fi

  osbe_cxx_parse_vars "${1}"

  { $as_echo "$as_me:$LINENO: parsing internal ${_osbe_cxx_parse_def__name}.def..." >&5
$as_echo "$as_me: parsing internal ${_osbe_cxx_parse_def__name}.def..." >&6;}

  # execute def file and find dependencies
  osbe_cxx_def_cycle "${1}" "${_osbe_cxx_parse_def__name}"
  osbe_cxx_execute_def "${1}" "${_osbe_cxx_parse_def__target}"
  osbe_cxx_def_cycle "${1}" ""

  osbe_cxx_parse_vars "${1}"

  eval _osbe_cxx_parse_def__deps=\
"\"include Common.pre.rules
include \\\$(osbe_builddir)/config/CXX/CXX.pre.rules
\${_osbe_cxx_lib__${1}_deps}\""

  eval _osbe_cxx_parse_def__printable_name=\
"\"\${_osbe_cxx_lib__${1}_name}\""
  if test -n "${_osbe_cxx_parse_def__printable_name}"; then
    #library, not executable
    _osbe_cxx_parse_def__deps="${_osbe_cxx_parse_def__deps}

libname := ${_osbe_cxx_parse_def__name}"
    #link .def
    _osbe_cxx_parse_def__target_link="libdefs/${_osbe_cxx_parse_def__name}.def"
    rm -f "${_osbe_cxx_parse_def__target_link}"
    if test `$echo ${_osbe_cxx_parse_def__target} | cut -b 1` != "/"; then
      _osbe_cxx_parse_def__target="../${_osbe_cxx_parse_def__target}"
    fi
    ${LN_S} "${_osbe_cxx_parse_def__target}" \
"${_osbe_cxx_parse_def__target_link}"
    # determine type
    eval _osbe_cxx_parse_def__so_files=\
"\"\${_osbe_cxx_lib__${1}_so_files}\""
    eval _osbe_cxx_parse_def__ar_files=\
"\"\${_osbe_cxx_lib__${1}_ar_files}\""
    eval _osbe_cxx_parse_def__ld_flags=\
"\"\${_osbe_cxx_lib__${1}_ld_flags}\""
    _osbe_cxx_parse_def__type=
    _osbe_cxx_parse_def__post=
    if test -z "${_osbe_cxx_parse_def__so_files}"; then
      if test -z "${_osbe_cxx_parse_def__ar_files}"; then
        _osbe_cxx_parse_def__type=inl
        _osbe_cxx_parse_def__post="
include \$(osbe_builddir)/config/CXX/Inl.post.rules"
      else
        _osbe_cxx_parse_def__type=ar
        _osbe_cxx_parse_def__deps="${_osbe_cxx_parse_def__deps}
target := ${_osbe_cxx_parse_def__ar_files}"
        _osbe_cxx_parse_def__post="
include \$(osbe_builddir)/config/CXX/Ar.post.rules"
      fi
    else
      if test -z "${_osbe_cxx_parse_def__ar_files}"; then
        _osbe_cxx_parse_def__type=so
        _osbe_cxx_parse_def__deps="${_osbe_cxx_parse_def__deps}
target := ${_osbe_cxx_parse_def__so_files}"
        _osbe_cxx_parse_def__post="
include \$(osbe_builddir)/config/CXX/So.post.rules"
      else
        { { $as_echo "$as_me:$LINENO: error: \"${_osbe_cxx_parse_def__target}: \
both static and dynamic libraries are supplied\"" >&5
$as_echo "$as_me: error: \"${_osbe_cxx_parse_def__target}: \
both static and dynamic libraries are supplied\"" >&2;}
   { (exit 1); exit 1; }; }
      fi
    fi
    eval _osbe_cxx_defs_${1}_reference=\
"\"ifndef OSBE_MAKE_CLEAN_GOAL
include \\\$(top_builddir)/config/\${_osbe_cxx_parse_def__path}\${_osbe_cxx_parse_def__name}.\${_osbe_cxx_parse_def__type}.pre.rules
endif\""
  else
    #Executable, not a library. Target is unknown though.
    _osbe_cxx_parse_def__post="
include \$(osbe_builddir)/config/CXX/Ex.post.rules"
    eval _osbe_cxx_defs_${1}_reference="none"
  fi

  eval _osbe_cxx_def__deps_${1}=\
"\"\${_osbe_cxx_parse_def__deps}\""
  eval _osbe_cxx_def__post_${1}=\
"\"\${_osbe_cxx_lib__${1}_post}
\${_osbe_cxx_parse_def__post}\""
}

# Execute previously collected def files.
osbe_cxx_use_def_execution()
{
  for _osbe_cxx_use_def_execution__id in ${_osbe_cxx_defs_ids}; do
    osbe_cxx_parse_def "${_osbe_cxx_use_def_execution__id}"
  done
}

# Stores def ids and other info for later execution.
# ${1} - path to def file
# ${2} - its name
# ${3} - corresponding makefile (optional)
osbe_cxx_use_def()
{
  osbe_cxx_lib_name "${2}" _osbe_cxx_use_def__id

  eval _osbe_cxx_defs_${_osbe_cxx_use_def__id}_path="\"\${1}\""
  eval _osbe_cxx_defs_${_osbe_cxx_use_def__id}_name="\"\${2}\""

  eval _osbe_cxx_dependence_${_osbe_cxx_use_def__id}_makefile="\"\${1}\${3}\""

  _osbe_cxx_defs_ids="${_osbe_cxx_defs_ids} ${_osbe_cxx_use_def__id}"
}








if test -z "${_osbe_cxx_use_def__def_path}"; then
  if test -z "${OSBE_CXX_DEF_PATH}"; then
    _osbe_cxx_use_def__project_root=`(cd ${srcdir}; pwd)`
    _osbe_cxx_use_def__def_path=\
"${_osbe_cxx_use_def__project_root}/defs /usr/share/OpenSBE/defs"
  else
    _osbe_cxx_use_def__def_path="${OSBE_CXX_DEF_PATH}"
  fi

  osbe_config_mech_write_comment
  osbe_ask_for_ne_string "Please enter path(s) to external def files" \
                         "path(s) to external def files" \
                         _osbe_cxx_use_def__def_path \
                         "cxx.def.path"
fi

{ as_dir=libdefs
  case $as_dir in #(
  -*) as_dir=./$as_dir;;
  esac
  test -d "$as_dir" || { $as_mkdir_p && mkdir -p "$as_dir"; } || {
    as_dirs=
    while :; do
      case $as_dir in #(
      *\'*) as_qdir=`$as_echo "$as_dir" | sed "s/'/'\\\\\\\\''/g"`;; #'(
      *) as_qdir=$as_dir;;
      esac
      as_dirs="'$as_qdir' $as_dirs"
      as_dir=`$as_dirname -- "$as_dir" ||
$as_expr X"$as_dir" : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X"$as_dir" : 'X\(//\)[^/]' \| \
	 X"$as_dir" : 'X\(//\)$' \| \
	 X"$as_dir" : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X"$as_dir" |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
      test -d "$as_dir" && break
    done
    test -z "$as_dirs" || eval "mkdir $as_dirs"
  } || test -d "$as_dir" || { { $as_echo "$as_me:$LINENO: error: cannot create directory $as_dir" >&5
$as_echo "$as_me: error: cannot create directory $as_dir" >&2;}
   { (exit 1); exit 1; }; }; }
rm -rf libdefs/*.def





# ${1} - type
osbe_cxx_generate_rule()
{
  _osbe_cxx_generate_rule__rule="\
dep1 := \$(empty) ${_osbe_cxx_auto_dep_generate__dependent_element} \$(empty)
dep2 := \$(empty) ${_osbe_cxx_auto_dep_generate__dependence_element} \$(empty)
ifeq (\$(dep1)\$(dep2),\
\$(findstring \$(dep1),\$(dep))\$(findstring \$(dep2),\$(dep)))
${_osbe_cxx_auto_dep_generate__dependent_element} : \
${_osbe_cxx_auto_dep_generate__dependence_element}
endif"
  eval _osbe_cxx_auto_dep_generate__common_root_${_osbe_cxx_auto_dep_generate__common_root_id}_${1}_rules=\
"\"\${_osbe_cxx_auto_dep_generate__common_root_${_osbe_cxx_auto_dep_generate__common_root_id}_${1}_rules}
\${_osbe_cxx_generate_rule__rule}\""

  eval _osbe_cxx_generate_rule__common_root_present=\
"\"\${_osbe_cxx_auto_dep_generate__common_root_${_osbe_cxx_auto_dep_generate__common_root_id}_${1}_present}\""
  if test -z "${_osbe_cxx_generate_rule__common_root_present}"; then
    eval _osbe_cxx_auto_dep_generate__common_root_${_osbe_cxx_auto_dep_generate__common_root_id}_${1}_present=1
    eval _osbe_cxx_auto_dep_generate__common_root_${_osbe_cxx_auto_dep_generate__common_root_id}_path=\
"\${_osbe_cxx_auto_dep_generate__common_root}"
    eval _osbe_cxx_auto_dep_generate__all_${1}_common_roots=\
"\"\${_osbe_cxx_auto_dep_generate__all_${1}_common_roots} \
\${_osbe_cxx_auto_dep_generate__common_root_id}\""
  fi
}




# ${1} - type
# ${2} - target variable
osbe_cxx_combine_all_rules()
{
  eval _osbe_cxx_combine_all_rules__all_roots=\
"\"\${_osbe_cxx_auto_dep_generate__all_${1}_common_roots}\""
  _osbe_cxx_combine_all_rules__all_rules=\
"empty :=
dep := \$(empty) \$(sort \$(target_${1}_list)) \$(empty)
"
  for _osbe_cxx_combine_all_rules__common_root_id in \
${_osbe_cxx_combine_all_rules__all_roots}; do
    eval _osbe_cxx_combine_all_rules__common_root=\
"\"\${_osbe_cxx_auto_dep_generate__common_root_${_osbe_cxx_combine_all_rules__common_root_id}_path}\""
    if [ "X$_osbe_cxx_combine_all_rules__common_root" = "X" ]; then
      _osbe_cxx_combine_all_rules__common_root=/
    fi
    eval _osbe_cxx_combine_all_rules__common_root_rule=\
"\"\${_osbe_cxx_auto_dep_generate__common_root_${_osbe_cxx_combine_all_rules__common_root_id}_${1}_rules}\""
    _osbe_cxx_combine_all_rules__all_rules=\
"${_osbe_cxx_combine_all_rules__all_rules}

# Dependences in /${_osbe_cxx_combine_all_rules__common_root} directory
ifeq (\$(reldir)/,/${_osbe_cxx_combine_all_rules__common_root})\
${_osbe_cxx_combine_all_rules__common_root_rule}
endif"
  done
  eval ${2}=\
"\"ifneq (\\\$(auto_dependences),off)
\${_osbe_cxx_combine_all_rules__all_rules}
endif\""
}




osbe_cxx_auto_dep_generate()
{
  { $as_echo "$as_me:$LINENO: generating automatic dependencies..." >&5
$as_echo "$as_me: generating automatic dependencies..." >&6;}
  # all dependent
  for _osbe_cxx_auto_dep_generate__dependent in \
${_osbe_cxx_defs_ids}; do
    # its makefile and dependences
    eval _osbe_cxx_auto_dep_generate__dependent_makefile=\
"\"\${_osbe_cxx_dependence_${_osbe_cxx_auto_dep_generate__dependent}_makefile}\""
    eval _osbe_cxx_auto_dep_generate__dependences=\
"\"\${_osbe_cxx_dependence_${_osbe_cxx_auto_dep_generate__dependent}_ids}\""
    # all dependences of the dependent
    for _osbe_cxx_auto_dep_generate__dependence in \
${_osbe_cxx_auto_dep_generate__dependences}; do
      # makefile of the dependence
      eval _osbe_cxx_auto_dep_generate__dependence_makefile=\
"\"\${_osbe_cxx_dependence_${_osbe_cxx_auto_dep_generate__dependence}_makefile}\""
      if test -z "${_osbe_cxx_auto_dep_generate__dependence_makefile}"; then
        continue
      fi

      if test "${_osbe_cxx_auto_dep_generate__dependent_makefile}" = \
"${_osbe_cxx_auto_dep_generate__dependence_makefile}"; then
        { $as_echo "$as_me:$LINENO: WARNING: Unable to determine dependence for \
${_osbe_cxx_auto_dep_generate__dependent} and \
${_osbe_cxx_auto_dep_generate__dependence} because they use the same makefile \
${_osbe_cxx_auto_dep_generate__dependent_makefile}" >&5
$as_echo "$as_me: WARNING: Unable to determine dependence for \
${_osbe_cxx_auto_dep_generate__dependent} and \
${_osbe_cxx_auto_dep_generate__dependence} because they use the same makefile \
${_osbe_cxx_auto_dep_generate__dependent_makefile}" >&2;}
        continue
      fi

      # find common root of two makefiles
      _osbe_cxx_auto_dep_generate__common_root=`perl -e '\
sub min($$) { return $_[0] < $_[1] ? $_[0] : $_[1]; } \
my $pos = min(length($ARGV[0]), length($ARGV[1])); \
for (1 .. $pos) \
{ \
  if (substr($ARGV[0], 0, $_) ne substr($ARGV[1], 0, $_)) \
  { $pos = $_ - 1; last; } \
} \
while ($pos && substr($ARGV[0], $pos - 1, 1) ne "/") { $pos--; } \
print substr($ARGV[0], 0, $pos); \
' "${_osbe_cxx_auto_dep_generate__dependent_makefile}" \
"${_osbe_cxx_auto_dep_generate__dependence_makefile}"`

      # find dependency elements of two makefiles in the common root
      _osbe_cxx_auto_dep_generate__dependent_element=`perl -e \
'$_ = substr($ARGV[0], length($ARGV[1])); /[^\/]*(\/|$)/; print $&;' \
"${_osbe_cxx_auto_dep_generate__dependent_makefile}" \
"${_osbe_cxx_auto_dep_generate__common_root}"`
      _osbe_cxx_auto_dep_generate__dependence_element=`perl -e \
'$_ = substr($ARGV[0], length($ARGV[1])); /[^\/]*(\/|$)/; print $&;' \
"${_osbe_cxx_auto_dep_generate__dependence_makefile}" \
"${_osbe_cxx_auto_dep_generate__common_root}"`
      osbe_cxx_lib_name "_${_osbe_cxx_auto_dep_generate__common_root}" \
_osbe_cxx_auto_dep_generate__common_root_id

      if test -z "${_osbe_cxx_auto_dep_generate__dependent_element}"; then
        { $as_echo "$as_me:$LINENO: WARNING: Unable to determine dependence for \
${_osbe_cxx_auto_dep_generate__dependent} and \
${_osbe_cxx_auto_dep_generate__dependence} because the second \
${_osbe_cxx_auto_dep_generate__dependence_makefile} is a child of \
the first ${_osbe_cxx_auto_dep_generate__dependent_makefile}" >&5
$as_echo "$as_me: WARNING: Unable to determine dependence for \
${_osbe_cxx_auto_dep_generate__dependent} and \
${_osbe_cxx_auto_dep_generate__dependence} because the second \
${_osbe_cxx_auto_dep_generate__dependence_makefile} is a child of \
the first ${_osbe_cxx_auto_dep_generate__dependent_makefile}" >&2;}
        continue
      fi
      if test -z "${_osbe_cxx_auto_dep_generate__dependence_element}"; then
        { $as_echo "$as_me:$LINENO: WARNING: Unable to determine dependence for \
${_osbe_cxx_auto_dep_generate__dependent} and \
${_osbe_cxx_auto_dep_generate__dependence} because dependence \
${_osbe_cxx_auto_dep_generate__dependence_makefile} is a parent of \
dependent ${_osbe_cxx_auto_dep_generate__dependent_makefile}" >&5
$as_echo "$as_me: WARNING: Unable to determine dependence for \
${_osbe_cxx_auto_dep_generate__dependent} and \
${_osbe_cxx_auto_dep_generate__dependence} because dependence \
${_osbe_cxx_auto_dep_generate__dependence_makefile} is a parent of \
dependent ${_osbe_cxx_auto_dep_generate__dependent_makefile}" >&2;}
        continue
      fi

      # check for repeating
      osbe_cxx_lib_name "${_osbe_cxx_auto_dep_generate__dependent_element}" \
_osbe_cxx_auto_dep_generate__dependent_element_id
      osbe_cxx_lib_name "${_osbe_cxx_auto_dep_generate__dependence_element}" \
_osbe_cxx_auto_dep_generate__dependence_element_id

      eval _osbe_cxx_auto_dep_generate__rule_processed=\
"\"\${_osbe_cxx_auto_dep_generate__rule__${_osbe_cxx_auto_dep_generate__common_root_id}__${_osbe_cxx_auto_dep_generate__dependent_element_id}__${_osbe_cxx_auto_dep_generate__dependence_element_id}__processed}\""
      if test -n "${_osbe_cxx_auto_dep_generate__rule_processed}"; then
        continue
      fi
      eval "_osbe_cxx_auto_dep_generate__rule__${_osbe_cxx_auto_dep_generate__common_root_id}__${_osbe_cxx_auto_dep_generate__dependent_element_id}__${_osbe_cxx_auto_dep_generate__dependence_element_id}__processed=1"

      # determine types of dependency elements. they must coincide.
      _osbe_cxx_auto_dep_generate__dependent_last=`perl -e \
'print substr($ARGV[0], length($ARGV[0]) - 1)' \
"${_osbe_cxx_auto_dep_generate__dependent_element}"`
      _osbe_cxx_auto_dep_generate__dependence_last=`perl -e \
'print substr($ARGV[0], length($ARGV[0]) - 1)' \
"${_osbe_cxx_auto_dep_generate__dependence_element}"`

      if test "${_osbe_cxx_auto_dep_generate__dependent_last}" = "/"; then
        if test "${_osbe_cxx_auto_dep_generate__dependence_last}" = "/"; then
          _osbe_cxx_auto_dep_generate__dependent_element=`perl -e \
'print substr($ARGV[0], 0, length($ARGV[0]) - 1)' \
"${_osbe_cxx_auto_dep_generate__dependent_element}"`
          _osbe_cxx_auto_dep_generate__dependence_element=`perl -e \
'print substr($ARGV[0], 0, length($ARGV[0]) - 1)' \
"${_osbe_cxx_auto_dep_generate__dependence_element}"`
          # Dependency between subdirectories
          osbe_cxx_generate_rule "directory"
        else
          { $as_echo "$as_me:$LINENO: WARNING: Unable to determine dependence for \
${_osbe_cxx_auto_dep_generate__dependent} and \
${_osbe_cxx_auto_dep_generate__dependence} because the first \
${_osbe_cxx_auto_dep_generate__dependent_makefile} is a directory but \
the second ${_osbe_cxx_auto_dep_generate__dependence_makefile} is not" >&5
$as_echo "$as_me: WARNING: Unable to determine dependence for \
${_osbe_cxx_auto_dep_generate__dependent} and \
${_osbe_cxx_auto_dep_generate__dependence} because the first \
${_osbe_cxx_auto_dep_generate__dependent_makefile} is a directory but \
the second ${_osbe_cxx_auto_dep_generate__dependence_makefile} is not" >&2;}
          continue
        fi
      else
        if test "${_osbe_cxx_auto_dep_generate__dependence_last}" = "/"; then
          { $as_echo "$as_me:$LINENO: WARNING: Unable to determine dependence for \
${_osbe_cxx_auto_dep_generate__dependent} and \
${_osbe_cxx_auto_dep_generate__dependence} because the second \
${_osbe_cxx_auto_dep_generate__dependence_makefile} is a directory but \
the first ${_osbe_cxx_auto_dep_generate__dependent_makefile} is not" >&5
$as_echo "$as_me: WARNING: Unable to determine dependence for \
${_osbe_cxx_auto_dep_generate__dependent} and \
${_osbe_cxx_auto_dep_generate__dependence} because the second \
${_osbe_cxx_auto_dep_generate__dependence_makefile} is a directory but \
the first ${_osbe_cxx_auto_dep_generate__dependent_makefile} is not" >&2;}
          continue
        else
          # Dependency between makefiles
          osbe_cxx_generate_rule "makefile"
        fi
      fi
    done
  done

  # combine generated rules
  osbe_cxx_combine_all_rules "directory" \
"OSBE_DIRENTRY_AUTO_DEPENDENCIES_VAR"
  osbe_cxx_combine_all_rules "makefile" \
"OSBE_MAKENTRY_AUTO_DEPENDENCIES_VAR"
}









