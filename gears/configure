  osbe_config_mech_out_works="1"
  { $as_echo "$as_me:$LINENO: $osbe_config_mech_signature : output config file : \
$osbe_config_mech_out_file" >&5
$as_echo "$as_me: $osbe_config_mech_signature : output config file : \
$osbe_config_mech_out_file" >&6;}
fi

#
# write newline
#
osbe_config_mech_write_nl()
{
  if test "$osbe_config_mech_out_works" = "1"; then
    $echo  >> $osbe_config_mech_out_file
  fi
}

#
# $1 is a comment text
#
osbe_config_mech_write_comment()
{
  if test "$osbe_config_mech_out_works" = "1"; then
    $echo "# $1"   >> $osbe_config_mech_out_file
  fi
}

#
# $1 is a key name (e.g. "cxx.id")
# $2 is a value    (e.g. "ggc")
#
osbe_config_mech_write_value()
{
  if test "$osbe_config_mech_out_works" = "1"; then
    $echo "$1 = $2" | sed -e 's/['\''$"\\`]/\\&/g'  >> $osbe_config_mech_out_file
  fi
}

#
# $1 is a key name (e.g. "cxx.id")
# $2 is a name of the variable that will accept value
# Note: if the mech is disabled or key is not found then
# the previous content is not modified
#
osbe_config_mech_read_value()
{
  if test "$osbe_config_mech_in_works" = "1"; then
    _osbe_config_mech_read_value_escaped_key="$1"
    _osbe_config_mech_read_value_sed_script=\
"\%^ *$_osbe_config_mech_read_value_escaped_key%{;\
s%^ *$_osbe_config_mech_read_value_escaped_key *= *\(.*\)%\1%p;q}
\$i\\
_KEY_NOT_FOUND_"

  _osbe_config_mech_read_value_temp=\
`sed -n -e "$_osbe_config_mech_read_value_sed_script" $osbe_config_mech_in_file`

    if test "$_osbe_config_mech_read_value_temp" != "_KEY_NOT_FOUND_"; then
      eval $2="\"$_osbe_config_mech_read_value_temp\""
    fi
  fi
}


#
# Check if variable is not defined
#
# $1 - name of variable to be checked
#
osbe_var_undefined()
{
  _osbe_var_undefined__not_defined="osbe: the variable is not defined"

  eval _osbe_var_undefined__var_value=\"\${$1-"$_osbe_var_undefined__not_defined"}\"

  if test "$_osbe_var_undefined__var_value" = \
          "$_osbe_var_undefined__not_defined"; then
    $echo "1"
  else
    $echo "0"
  fi
}

#
# Space separated list size
#
# $1 - space separated list
#

osbe_list_size()
{
  osbe_list_size__size="0"

  for osbe_list_size__item in $1; do
    osbe_list_size__size=`expr $osbe_list_size__size + 1`
  done

  $echo "$osbe_list_size__size"
}

#
# Space separated list item
#
# $1 - space separated list
# $2 - item index
#

osbe_list_item()
{
  osbe_list_item__size="0"

  for osbe_list_item__item in $1; do

    if test "$osbe_list_item__size" -eq "$2"; then
      $echo "$osbe_list_item__item"
      return 0
    fi

    osbe_list_item__size=`expr $osbe_list_item__size + 1`
  done

  $echo ""
}

#
# Aggregate lists of strings.
# Return "A B E : C D E : A B F : C D F :" for
# parameters "A B : C D :" and "E F"
#
# $1 - aggregated ":" separated list of space separated lists
# $2 - space separated list to aggregate
#
osbe_list_aggregate()
{
  _osbe_list_aggregate__aggregated_list="$1"
  _osbe_list_aggregate__result=""
  _osbe_list_aggregate__already_present="0"
  _osbe_list_aggregate__new_list=""

  if test -z "$_osbe_list_aggregate__aggregated_list"; then
    _osbe_list_aggregate__aggregated_list=":"
  fi

  for _osbe_list_aggregate__list_item in $2; do

    for _osbe_list_aggregate__aggregated_list_item in \
        $_osbe_list_aggregate__aggregated_list; do
      if test "$_osbe_list_aggregate__aggregated_list_item" = ":"; then

        if test "$_osbe_list_aggregate__already_present" = "0"; then
          _osbe_list_aggregate__new_list=\
"$_osbe_list_aggregate__new_list$_osbe_list_aggregate__list_item "
        fi

        _osbe_list_aggregate__new_list_present="0"

        _osbe_list_aggregate__cur_list=""
        for _osbe_list_aggregate__cur_list_item in \
            $_osbe_list_aggregate__result; do
          if test "$_osbe_list_aggregate__cur_list_item" = ":"; then
            if test "$_osbe_list_aggregate__new_list" = \
                "$_osbe_list_aggregate__cur_list"; then
              _osbe_list_aggregate__new_list_present="1"
            fi
            _osbe_list_aggregate__cur_list=""
          else
            _osbe_list_aggregate__cur_list=\
"$_osbe_list_aggregate__cur_list$_osbe_list_aggregate__cur_list_item "
          fi
        done

        if test "$_osbe_list_aggregate__new_list_present" = "0"; then
          _osbe_list_aggregate__result=\
"$_osbe_list_aggregate__result$_osbe_list_aggregate__new_list: "
        fi

        _osbe_list_aggregate__already_present="0"
        _osbe_list_aggregate__new_list=""

      else

        if test "$_osbe_list_aggregate__aggregated_list_item" = \
           "$_osbe_list_aggregate__list_item"; then
          _osbe_list_aggregate__already_present="1"
        fi

        _osbe_list_aggregate__new_list=\
"$_osbe_list_aggregate__new_list$_osbe_list_aggregate__aggregated_list_item "

      fi
    done

  done

  $echo "$_osbe_list_aggregate__result"
}

#
# Prefix words
#
# $1 - prefix needed to be added
# $2 - space separated list of words to be prefixed
#
osbe_prefix_words()
{
  osbe_prefix_words__prefix=$1
  osbe_prefix_words__string=$2
  osbe_prefix_words__result=""
  for word in $osbe_prefix_words__string; do
    if test -n "$osbe_prefix_words__result"; then
      osbe_prefix_words__result=\
"$osbe_prefix_words__result $osbe_prefix_words__prefix$word"
    else
      osbe_prefix_words__result="$osbe_prefix_words__prefix$word"
    fi
  done

  $echo "$osbe_prefix_words__result"
}

#
# Suffix words
#
# $1 - suffix needed to be added
# $2 - space separated list of words to be suffixed
#
osbe_suffix_words()
{
  osbe_suffix_words__suffix=$1
  osbe_suffix_words__string=$2
  osbe_suffix_words__result=""
  for word in $osbe_suffix_words__string; do
    if test -n "$osbe_suffix_words__result"; then
      osbe_suffix_words__result=\
"$osbe_suffix_words__result $word$osbe_suffix_words__suffix"
    else
      osbe_suffix_words__result="$word$osbe_suffix_words__suffix"
    fi
  done

  $echo "$osbe_suffix_words__result"
}

#
# Wrap words both sides
#
# $1 - prefix needed to be added
# $2 - suffix needed to be added
# $3 - space separated list of words to be wrapped
#
osbe_wrap_words()
{
  osbe_wrap_words__prefixed=`osbe_prefix_words "$1" "$3"`
  osbe_suffix_words "$2" "$osbe_wrap_words__prefixed"
}

#
# Converts relative path to absolute
#
# $1 - path
#
osbe_rel2abs()
{
  osbe_rel2abs__path=$1
  osbe_rel2abs__dir=`dirname $1`
  osbe_rel2abs__base=`basename $1`
  osbe_rel2abs__abs_dir="`cd \"$osbe_rel2abs__dir\" 2>/dev/null && pwd`"
  if test -n "$osbe_rel2abs__abs_dir"; then
    $echo "$osbe_rel2abs__abs_dir/$osbe_rel2abs__base"
  fi
}

#
# Adds library path to a list will be used later for running commands
# in executing "check" make target
#
# $1 - library path
#
osbe_library_path()
{
  for _path in ${_osbe_library_paths}; do
    if [ "${_path}" = "${1}" ] ; then
      return
    fi
  done
  _osbe_library_paths="${_osbe_library_paths} ${1}"
}

#
# Adds library path from ld flags to a list will be used later for
# running commands in executing "check" make target
#
# $1 - ld flags
#
osbe_ld_flags_to_library_path()
{
#  for _osbe_option in $1; do
#    if test "${_osbe_option:0:2}" = "-L"; then
#      osbe_library_path "${_osbe_option:2}"
#    fi
#  done

  for _osbe_option in $1; do
    _osbe_tmp=`$echo "$_osbe_option"|sed -n "s/^\(-L\).*/\1/p"`
    if test "$_osbe_tmp" = "-L"; then
      _osbe_tmp=`$echo "$_osbe_option"|sed -n "s/^-L\(.*\)/\1/p"`
      osbe_library_path "$_osbe_tmp"
    fi
  done
}

