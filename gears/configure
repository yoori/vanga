osbe_config_mech_write_comment

cxx_known_ids=""

for def_file in ${srcdir}/OpenSBE/config/CXX/Defaults/*.def; do

  cxx_identifier=`$echo "$def_file"|sed -n -e "s%.*/\(.*\)\.def%\1%p"`
  cxx_platforms=""
  cxx_default_for_platforms=""

  . $def_file

  eval cxx_known_name_$cxx_identifier="\"${cxx_known_name}\""

  for p in $cxx_platforms; do
    pattern=`$echo "s|$p|&|p"|sed -e "s/%/.*/g"`
    test_result=`$echo $host_os|sed -n -e "$pattern"`

    if test "$test_result" = "$host_os"; then
      if test "$cxx_identifier" = "Other"; then
        cxx_append_id="Other"
      else
        cxx_known_ids="$cxx_known_ids $cxx_identifier"

        if test -z "$CXX_ID"; then
          for d in $cxx_default_for_platforms; do
            pattern=`$echo "s|$d|&|p"|sed -e "s/%/.*/g"`
            test_result=`$echo $host_os|sed -n -e "$pattern"`

            if test "$test_result" = "$host_os"; then
              CXX_ID=$cxx_identifier
            fi
          done
        fi
      fi

      break
    fi
  done

done

if test -z "$CXX_ID"; then
  CXX_ID="Other"
fi

