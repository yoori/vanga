    "${_osbe_external_lib_key_base}.root" \
    "1"

# if root dir is not empty, then suggest to accept predefined values
#   if accept we are done
#   else suggest to set every value separately with defaults
# else suggest to set every value (with no defaults for include and lib dirs)

  eval _osbe_external_so_files="\"\$$_osbe_default_so_files_var_name\""
  eval _osbe_external_ar_files="\"\$$_osbe_default_ar_files_var_name\""
  eval _osbe_external_ld_flags="\"\$$_osbe_default_ld_flags_var_name\""

  eval _osbe_default_include="\"\$$_osbe_default_include_var_name\""
  if test "$_osbe_default_include" != ""; then
    _osbe_external_lib_include_required="yes"
  else
    _osbe_external_lib_include_required="no"
  fi

  eval _osbe_default_lib="\"\$$_osbe_default_lib_var_name\""
  if test "$_osbe_default_lib" != ""; then
